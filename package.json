{
  "name": "se_project_express",
  "version": "1.0.0",
  "description": "A RESTful API for the WTWR app, built with Express and MongoDB",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint . --ext .js,.cjs,.mjs",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs --fix",
    "seed": "node scripts/seed.js",
    "ensure:index": "node scripts/ensure-email-unique-index.js",
    "test:api": "newman run WTWR_API.postman_collection.json -e WTWR-Local.postman_environment.json --reporters cli,htmlextra --reporter-htmlextra-export newman/wtwr-sprint14.html --reporter-htmlextra-title \"WTWR Sprint 14 - API Tests\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tripleten-com/se_project_express.git"
  },
  "keywords": [
    "API",
    "WTWR"
  ],
  "author": "Fred Hobbs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tripleten-com/se_project_express/issues"
  },
  "homepage": "https://github.com/tripleten-com/se_project_express#readme",
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "celebrate": "^15.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "validator": "^13.15.15"
  }
}
